{
  "name": "angular-component-library-p",
  "version": "1.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:lib": "ng build angular-component-library --configuration production",
    "build:lib-watch": "ng build angular-component-library --configuration production --watch",
    "pack:lib": "cd dist/angular-component-library && npm pack",
    "link:lib": "cd dist/angular-component-library && npm link",
    "test": "ng test",
    "prettier": "npx prettier --write .",
    "storybook": "ng run angular-component-library:storybook",
    "test-storybook": "test-storybook --config-dir projects/angular-component-library/.storybook --url http://localhost:6006",
    "build-storybook": "ng run angular-component-library:build-storybook",
    "update:devkit": "ng update @angular-devkit/build-angular",
    "update:angular": "ng update @angular/cli@19 @angular/core@19",
    "update:angular/cdk": "ng update @angular/cdk@19 @ngx-translate/core",
    "update:fortawesome": "ng update @fortawesome/angular-fontawesome@1 @fortawesome/fontawesome-svg-core @fortawesome/free-brands-svg-icons @fortawesome/free-regular-svg-icons @fortawesome/free-solid-svg-icons",
    "update:util": "ng update date-fns @types/lodash.clonedeep lodash.clonedeep luxon @types/luxon rxjs tslib zone.js prettier",
    "update:karma": "ng update karma karma-chrome-launcher karma-coverage karma-jasmine karma-jasmine-html-reporter",
    "update:storybook": "ng update @storybook/addon-a11y @storybook/addon-docs @storybook/addon-essentials @storybook/addon-interactions @storybook/addon-onboarding @storybook/angular @storybook/blocks @storybook/test @storybook/test-runner storybook storybook-design-token @compodoc/compodoc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.15",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.15",
    "@angular/compiler": "^19.2.15",
    "@angular/core": "^19.2.15",
    "@angular/forms": "^19.2.15",
    "@angular/platform-browser": "^19.2.15",
    "@angular/platform-browser-dynamic": "^19.2.15",
    "@angular/router": "^19.2.15",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@ngx-translate/core": "^17.0.0",
    "@storybook/addon-a11y": "^8.6.12",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/luxon": "^3.7.1",
    "date-fns": "^4.1.0",
    "lodash.clonedeep": "^4.5.0",
    "luxon": "^3.7.2",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.17",
    "@angular/cli": "^19.2.17",
    "@angular/compiler-cli": "^19.2.15",
    "@compodoc/compodoc": "^1.1.26",
    "@storybook/addon-docs": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/angular": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/test-runner": "^0.22.0",
    "@types/jasmine": "~5.1.0",
    "@types/webpack": "^5.28.5",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.2",
    "playwright": "^1.52.0",
    "prettier": "^3.6.2",
    "storybook": "^8.6.12",
    "storybook-design-token": "^3.2.0",
    "typescript": "~5.5.4"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
