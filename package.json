{
    "name": "angular-component-library-p",
    "version": "2.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --configuration production",
        "build:lib": "ng build angular-component-library --configuration production",
        "build:lib-watch": "ng build angular-component-library --configuration production --watch",
        "pack:lib": "cd dist/angular-component-library && npm pack",
        "link:lib": "cd dist/angular-component-library && npm link",
        "test": "ng test",
        "prettier": "npx prettier --write .",
        "storybook": "ng run angular-component-library:storybook",
        "test-storybook": "test-storybook --config-dir projects/angular-component-library/.storybook --url http://localhost:6006",
        "build-storybook": "ng run angular-component-library:build-storybook",
        "update:devkit": "ng update @angular-devkit/build-angular@20.0.0",
        "update:angular": "ng update @angular/cli@20 @angular/core@20",
        "update:angular/cdk": "ng update @angular/cdk@20 @ngx-translate/core",
        "update:fortawesome": "ng update @fortawesome/angular-fontawesome @fortawesome/fontawesome-svg-core @fortawesome/free-brands-svg-icons @fortawesome/free-regular-svg-icons @fortawesome/free-solid-svg-icons",
        "update:util": "ng update date-fns @types/lodash.clonedeep lodash.clonedeep luxon @types/luxon rxjs tslib zone.js prettier",
        "update:karma": "ng update karma karma-chrome-launcher karma-coverage karma-jasmine karma-jasmine-html-reporter",
        "update:storybook": "ng update @storybook/angular@^9 storybook@^9 @storybook/addon-docs@^9 @storybook/addon-onboarding@^9 @storybook/test-runner storybook-design-token @compodoc/compodoc"
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/build-angular": "^20.3.11",
        "@angular/animations": "^20.3.13",
        "@angular/cdk": "^20.2.14",
        "@angular/common": "^20.3.13",
        "@angular/compiler": "^20.3.13",
        "@angular/core": "^20.3.13",
        "@angular/forms": "^20.3.13",
        "@angular/platform-browser": "^20.3.13",
        "@angular/platform-browser-dynamic": "^20.3.13",
        "@angular/router": "^20.3.13",
        "@fortawesome/angular-fontawesome": "^3.0.0",
        "@fortawesome/fontawesome-svg-core": "^7.1.0",
        "@fortawesome/free-brands-svg-icons": "^7.1.0",
        "@fortawesome/free-regular-svg-icons": "^7.1.0",
        "@fortawesome/free-solid-svg-icons": "^7.1.0",
        "@ngx-translate/core": "^17.0.0",
        "@storybook/addon-a11y": "^10.0.8",
        "@storybook/builder-webpack5": "^10.0.8",
        "@types/lodash.clonedeep": "^4.5.9",
        "@types/luxon": "^3.7.1",
        "date-fns": "^4.1.0",
        "lodash.clonedeep": "^4.5.0",
        "luxon": "^3.7.2",
        "rxjs": "~7.8.2",
        "tslib": "^2.8.1",
        "zone.js": "~0.15.1"
    },
    "devDependencies": {
        "@angular/build": "^20.3.11",
        "@angular/cli": "^20.3.11",
        "@angular/compiler-cli": "^20.3.13",
        "@compodoc/compodoc": "^1.1.26",
        "@storybook/addon-docs": "^10.0.8",
        "@storybook/addon-onboarding": "^10.0.8",
        "@storybook/angular": "^10.0.8",
        "@storybook/test-runner": "^0.24.1",
        "@types/jasmine": "~5.1.0",
        "@types/webpack": "^5.28.5",
        "eslint-plugin-storybook": "10.0.8",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.3",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "ng-packagr": "^20.3.2",
        "playwright": "^1.52.0",
        "prettier": "^3.6.2",
        "storybook": "^10.0.8",
        "typescript": "~5.9.3"
    },
    "overrides": {
        "react": "18.2.0",
        "react-dom": "18.2.0"
    }
}
